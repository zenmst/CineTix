<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_movies">

    <fragment
        android:id="@+id/navigation_movies"
        android:name="com.uas.cinetix.ui.home.MoviesFragment"
        android:label="Movies"
        tools:layout="@layout/fragment_movies" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.uas.cinetix.ui.dashboard.DashboardFragment"
        android:label="Cinemas"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_tickets"
        android:name="com.uas.cinetix.ui.notifications.MyTicketFragment"
        android:label="My Tickets"
        tools:layout="@layout/fragment_my_ticket" />
</navigation>
